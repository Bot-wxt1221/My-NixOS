    <div class="am-modal-dialog">
        <div class="am-modal-hd" id="lg-alert-title"></div>
        <div class="am-modal-bd" id="lg-alert-message"></div>
        <div class="am-modal-footer">
            <span class="am-modal-btn">确定</span>
        </div>
    </div>
</div>

